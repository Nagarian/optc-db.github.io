{
  "name": "optc-db-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "old-extract": "ts-node old-extract.ts",
    "old-schema": "ts-json-schema-generator -f ./tsconfig.json -o './models/old-db-schema.json' --path './models/old-db.d.ts' --type 'OldDB.ExtendedUnit'",

    "extract": "ts-node extract.ts",
    "raw-schema": "npm run raw-schema:all && npm run raw-schema:basic && npm run raw-schema:dual && npm run raw-schema:versus",
    "raw-schema:all": "ts-json-schema-generator -f ./tsconfig.json -o './models/raw-db-schema.json' --path './models/raw-db.d.ts' --type 'RawDB.Character'",
    "raw-schema:basic": "ts-json-schema-generator -f ./tsconfig.json -o './models/raw-db-basic.schema.json' --path './models/raw-db.d.ts' --type 'RawDB.BasicCharacter'",
    "raw-schema:dual": "ts-json-schema-generator -f ./tsconfig.json -o './models/raw-db-dual.schema.json' --path './models/raw-db.d.ts' --type 'RawDB.DualCharacter'",
    "raw-schema:versus": "ts-json-schema-generator -f ./tsconfig.json -o './models/raw-db-versus.schema.json' --path './models/raw-db.d.ts' --type 'RawDB.VersusCharacter'"
  },
  "dependencies": {
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "ajv": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-standard": "^5.0.0",
    "ts-json-schema-generator": "^0.90.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
